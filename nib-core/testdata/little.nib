_ = print true
c = _prim_add 1 2
_ = print c
b = print "hello"

// Ok, let's be systematic
// Operators
a + b = _prim_add a b
a * b = _prim_mul a b
a - b = _prim_sub a b
a / b = _prim_div a b

// Function
inc x = x + 1

// Vararg
mkarr ...xs = xs

// Use
_ = print (mkarr 1 2 3 4 5)
ft = 2.9 + 1.0
_ = print ft
_ = print (inc 4)

//Projection
_ = print (nil_type.name)

// Recursion
fac 0 = 1
fac x = x * fac (x - 1)

_ = print (fac 4)

// Where
i = a + b where a=12;b=13

_ = print i

fib n = go 1 1 n
            where
                go _ b 0 = b
                go a b n = go b (a+b) (n-1)

_ = print (fib 5)
